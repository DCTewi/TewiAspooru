@{
    var TextCardPrev = "Card";
}

@model List<CardModel>;
@{
    ViewBag.Title = TextCardPrev + " " + Model[0].Pid;
    ViewBag.Full = 1;
}

<h1>@TextCardPrev @Model[0].Pid</h1>

@{
    for (int i = Model.Count - 1; i >= 0; i--)
    {
        @await Html.PartialAsync("_Card", Model[i]);
        <br />
    }
}
